{
  "name": "bezpecnost-ssps-web",
  "version": "1.12.4",
  "private": true,
  "repository": "https://github.com/EETagent/BezpecnostSSPS",
  "homepage": "https://bezpecnost.ssps.cz",
  "license": "MIT",
  "description": "Webová stránka pro akci HackDays a kybernetický polygon Smíchovské střední průmyslové školy a gymnázia",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-php": "vite & php -S localhost:8000 -t public",
    "start": "npm run dev",
    "start-php": "npm run dev-php",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-plugin-solid": "^0.8.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-solid": "^2.4.0"
  },
  "dependencies": {
    "fslightbox": "^3.3.1",
    "recaptcha-v3": "^1.10.0",
    "solid-app-router": "^0.4.2",
    "solid-js": "^1.6.2",
    "solid-transition-group": "^0.0.8"
  }
