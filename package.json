    "eslint-plugin-solid": "^0.12.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-solid": "^2.7.0"
  },
  "dependencies": {
    "fslightbox": "^3.4.1",
    "recaptcha-v3": "^1.10.0",
